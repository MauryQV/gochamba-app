{
  "expo": {
    "name": "GoChamba",
    "slug": "testing-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gochamba",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "2e045b6c-6c7a-4191-a1ae-e4ef22e92276"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gochamba.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gochamba.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.399496840515-jolv7lc6dr7rvag6ti7or7j3vp50s6gm"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci√≥n necesita acceso a tus fotos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2e045b6c-6c7a-4191-a1ae-e4ef22e92276"
    }
  }
}
